{
  "rules": [
    {
      "active": true,
      "proxyId": "direct",
      "name": "Bilibili bypass",
      "match": [
        "*://*.bilibili.tv/*"
      ],
      "bypassResourceTypes": [
        "font",
        "image",
        "imageset",
        "media",
        "object",
        "object_subrequest",
        "stylesheet",
        "web_manifest"
      ],
      "staticExtensions": "/\\.(jpg|jpeg|png|gif|webp|mp4|m4s|js|css|woff2?|ttf|svg|ico|json|xml)$/i",
      "fallbackDirect": true
    }
  ]
}
